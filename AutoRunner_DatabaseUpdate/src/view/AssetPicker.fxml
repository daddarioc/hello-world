<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="575.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AssetPickerController">

   <children>
      <TableView fx:id="tblAssets" prefHeight="497.0" prefWidth="868.0">
        <columns>
          <TableColumn fx:id="colAssetId" prefWidth="85.0" text="ID" />
          <TableColumn fx:id="colAssetName" prefWidth="580.0" text="Name" />
          <TableColumn fx:id="colStatus" minWidth="0.0" prefWidth="81.0" text="Status" />
          <TableColumn fx:id="colEndDate" prefWidth="103.0" text="End Date" />
        </columns>
      </TableView>
      <Button fx:id="btnNewVersion" mnemonicParsing="false" onAction="#assetNewVersion" text="Update Selected" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="750.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="btnBack" mnemonicParsing="false" onAction="#showProductPicker" text="Back" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="25.0" />
         </GridPane.margin>
      </Button>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="527.0" minHeight="512.0" prefHeight="512.0" />
      <RowConstraints maxHeight="50.0" minHeight="35.0" prefHeight="50.0" />
   </rowConstraints>
</GridPane>
