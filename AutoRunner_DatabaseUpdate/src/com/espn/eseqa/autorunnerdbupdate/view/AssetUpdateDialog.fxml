<AnchorPane minHeight="300.0" minWidth="450.0" prefHeight="374.0" prefWidth="581.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.espn.eseqa.autorunnerdbupdate.view.AssetUpdateController">
   <children>
      <GridPane layoutX="14.0" layoutY="14.0" prefHeight="295.0" prefWidth="553.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="# Assets to update" />
            <Label text="End Date" GridPane.rowIndex="1" />
            <Label text="New Asset Start Date" GridPane.rowIndex="2" />
            <Label text="New Asset Status" GridPane.rowIndex="3" />
            <DatePicker fx:id="calEndDate" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <DatePicker fx:id="calNewStartDate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ComboBox fx:id="cmbNewStatus" prefHeight="29.0" prefWidth="191.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="lblAssetCount" minHeight="20.0" minWidth="60.0" prefHeight="25.0" prefWidth="187.0" GridPane.columnIndex="1">
               <opaqueInsets>
                  <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
               </opaqueInsets>
            </Label>
         </children>
      </GridPane>
      <Button fx:id="btnFinish" layoutX="482.0" layoutY="320.0" mnemonicParsing="false" text="Update Assets" onAction="#buildSql" />
      <Button fx:id="btnCancel" layoutX="407.0" layoutY="320.0" mnemonicParsing="false" text="Cancel" onAction="#closeDialog"/>
      <Separator layoutX="14.0" layoutY="305.0" prefHeight="6.0" prefWidth="553.0" />
   </children>
</AnchorPane>
